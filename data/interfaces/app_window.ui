<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.92.1 -->
<interface>
  <!-- interface-name app_window.ui -->
  <requires lib="libadwaita" version="1.5"/>
  <requires lib="gtk" version="4.12"/>
  <template class="SysdMainAppWindow" parent="AdwApplicationWindow">
    <property name="default-height">720</property>
    <property name="default-width">1280</property>
    <child>
      <object class="AdwToastOverlay">
        <child>
          <object class="AdwToolbarView">
            <property name="content">
              <object class="GtkPaned">
                <child>
                  <object class="GtkPaned">
                    <property name="end-child">
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkBox">
                            <child>
                              <object class="GtkLabel">
                                <property name="label">Enabled:</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkSwitch"/>
                            </child>
                            <child>
                              <object class="GtkButton">
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">media-playback-start-symbolic</property>
                                    <property name="label">Start</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton">
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">process-stop</property>
                                    <property name="label">Stop</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton">
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">view-refresh</property>
                                    <property name="label">Restart</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </property>
                    <property name="start-child">
                      <object class="GtkBox">
                        <child>
                          <object class="GtkLabel">
                            <property name="label">place holder</property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </property>
            <child type="top">
              <object class="AdwHeaderBar">
                <property name="decoration-layout">Unit Name</property>
                <property name="title-widget">
                  <object class="AdwWindowTitle">
                    <property name="title">SysD Manager</property>
                  </object>
                </property>
                <child type="start">
                  <object class="GtkButton">
                    <property name="icon-name">system-search</property>
                    <property name="tooltip-text">Filter uniresults</property>
                  </object>
                </child>
                <child type="start">
                  <object class="GtkButton">
                    <child>
                      <object class="AdwButtonContent">
                        <property name="icon-name">view-refresh</property>
                        <property name="label">Refresh</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="start">
                  <object class="GtkLabel">
                    <property name="label">Un</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
