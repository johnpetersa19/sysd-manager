<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.92.1 -->
<interface>
  <!-- interface-name app_window.ui -->
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.5"/>
  <template class="SysdMainAppWindow" parent="AdwApplicationWindow">
    <property name="default-height">720</property>
    <property name="default-width">1280</property>
    <child>
      <object class="AdwToastOverlay" id="toast_overlay">
        <child>
          <object class="AdwToolbarView">
            <property name="content">
              <object class="GtkPaned">
                <child>
                  <object class="GtkPaned" id="paned">
                    <property name="end-child">
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="spacing">5</property>
                        <child>
                          <object class="GtkBox">
                            <property name="spacing">5</property>
                            <child>
                              <object class="GtkLabel">
                                <property name="label">Enabled:</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkSwitch" id="ablement_switch">
                                <property name="sensitive">False</property>
                                <property name="valign">baseline-center</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton" id="start_button">
                                <property name="sensitive">False</property>
                                <property name="tooltip-text">Start unit</property>
                                <signal name="clicked" handler="button_start_clicked" swapped="yes"/>
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">media-playback-start-symbolic</property>
                                    <property name="label">Start</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton" id="stop_button">
                                <property name="sensitive">False</property>
                                <signal name="clicked" handler="button_stop_clicked" swapped="yes"/>
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">process-stop</property>
                                    <property name="label">Stop</property>
                                    <property name="tooltip-text">Stop unit</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton" id="restart_button">
                                <property name="sensitive">False</property>
                                <property name="tooltip-text">Restart unit</property>
                                <signal name="clicked" handler="button_restart_clicked" swapped="yes"/>
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">view-refresh</property>
                                    <property name="label">Restart</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkButton" id="kill_button">
                                <property name="sensitive">False</property>
                                <signal name="clicked" handler="button_kill_clicked" swapped="yes"/>
                                <child>
                                  <object class="AdwButtonContent">
                                    <property name="icon-name">face-sick-symbolic</property>
                                    <property name="label">Kill</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="AdwOverlaySplitView" id="side_overlay">
                            <property name="collapsed">True</property>
                            <property name="content">
                              <object class="GtkBox">
                                <child>
                                  <object class="AdwViewSwitcher">
                                    <property name="policy">wide</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwViewStack" id="unit_stack">
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="child">
                                          <object class="UnitInfoPanel" id="unit_info_panel"/>
                                        </property>
                                        <property name="title">Unit Info</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="child">
                                          <object class="UnitFilePanel" id="unit_file_panel"/>
                                        </property>
                                        <property name="title">File</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="AdwViewStackPage">
                                        <property name="child">
                                          <object class="JournalPanel" id="unit_journal_panel"/>
                                        </property>
                                        <property name="title">Journal</property>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </property>
                            <property name="sidebar">
                              <object class="KillPanel" id="kill_panel"/>
                            </property>
                            <property name="sidebar-position">end</property>
                          </object>
                        </child>
                      </object>
                    </property>
                    <property name="position-set">True</property>
                    <property name="shrink-start-child">False</property>
                    <property name="start-child">
                      <object class="UnitListPanel" id="unit_list_panel"/>
                    </property>
                    <property name="wide-handle">True</property>
                  </object>
                </child>
              </object>
            </property>
            <child type="top">
              <object class="AdwHeaderBar" id="header_bar">
                <property name="title-widget">
                  <object class="AdwWindowTitle">
                    <property name="title">SysD Manager</property>
                  </object>
                </property>
                <child>
                  <object class="GtkToggleButton">
                    <property name="icon-name">system-search</property>
                    <property name="tooltip-text">Open search bar</property>
                    <signal name="toggled" handler="button_search_toggled" swapped="yes"/>
                  </object>
                </child>
                <child type="start">
                  <object class="GtkButton">
                    <property name="icon-name">view-refresh</property>
                    <property name="tooltip-text">Refresh unit list</property>
                  </object>
                </child>
                <child type="start">
                  <object class="GtkLabel" id="unit_name_label">
                    <property name="label">Unit name</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
