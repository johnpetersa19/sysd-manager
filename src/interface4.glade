<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkPopoverMenu" id="unit_menu_popover">
    <property name="child">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkButton" id="services_button">
            <property name="label" translatable="1">Services</property>
            <property name="focusable">1</property>
            <property name="receives_default">1</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="sockets_button">
            <property name="label" translatable="1">Sockets</property>
            <property name="focusable">1</property>
            <property name="receives_default">1</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="timers_button">
            <property name="label" translatable="1">Timers</property>
            <property name="focusable">1</property>
            <property name="receives_default">1</property>
          </object>
        </child>
      </object>
<!--       <packing>
        <property name="submenu">main</property>
      </packing> -->
    </property>
  </object>
  <object class="GtkWindow" id="main_window">
    <property name="default_width">1000</property>
    <property name="default_height">600</property>
    <property name="icon_name">preferences-system</property>
    <property name="child">
      <object class="GtkBox">
        <child>
          <object class="GtkBox" id="left_pane">
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkStack" id="unit_stack">
                <property name="vexpand">1</property>
                <property name="transition_type">crossfade</property>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">Services</property>
                    <property name="title" translatable="1">Services</property>
                    <property name="child">
                      <object class="GtkScrolledWindow" id="services_window">
                        <property name="name">Services</property>
                        <property name="focusable">1</property>
                        <property name="hscrollbar_policy">never</property>
                      <!--   <property name="shadow_type">in</property> -->
                        <property name="child">
                          <object class="GtkViewport" id="services_viewport">
                            <property name="child">
                              <object class="GtkListBox" id="services_list"/>
                            </property>
                          </object>
                        </property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">Sockets</property>
                    <property name="title" translatable="1">Sockets</property>
                    <!--    <property name="position">1</property> -->
                    <property name="child">
                      <object class="GtkScrolledWindow" id="sockets_window">
                        <property name="name">Sockets</property>
                        <property name="focusable">1</property>
                        <property name="hscrollbar_policy">never</property>
                    <!--     <property name="shadow_type">in</property> -->
                        <property name="child">
                          <object class="GtkViewport" id="sockets_viewport">
                            <property name="child">
                              <object class="GtkListBox" id="sockets_list"/>
                            </property>
                          </object>
                        </property>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">Timers</property>
                    <property name="title" translatable="1">Timers</property>
              <!--       <property name="position">2</property> -->
                    <property name="child">
                      <object class="GtkScrolledWindow" id="timers_window">
                        <property name="name">Timers</property>
                        <property name="focusable">1</property>
                        <property name="hscrollbar_policy">never</property>
                    <!--     <property name="shadow_type">in</property> -->
                        <property name="child">
                          <object class="GtkViewport" id="timers_viewport">
                            <property name="child">
                              <object class="GtkListBox" id="timers_list"/>
                            </property>
                          </object>
                        </property>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="right_pane">
            <property name="hexpand">1</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkStackSwitcher">
                <property name="stack">info_stack</property>
              </object>
            </child>
            <child>
              <object class="GtkStack" id="info_stack">
                <property name="vexpand">1</property>
                <property name="transition_type">crossfade</property>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">Unit File</property>
                    <property name="title" translatable="1">Unit File</property>
                    <property name="child">
                      <object class="GtkBox" id="unit_file_stack">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="vexpand">1</property>
                            <property name="focusable">1</property>
                        <!--     <property name="shadow_type">in</property> -->
                            <property name="child">
                              <object class="GtkTextView" id="unit_info">
                                <property name="focusable">1</property>
                                <property name="wrap_mode">word-char</property>
                                <property name="left_margin">5</property>
                                <property name="right_margin">5</property>
                                <property name="monospace">1</property>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="save_button">
                            <property name="label">gtk-save</property>
                            <property name="focusable">1</property>
                            <property name="receives_default">1</property>
                    <!--         <property name="use_stock">True</property> -->
                          <!--   <property name="always_show_image">True</property> -->
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">Unit Journal</property>
                    <property name="title" translatable="1">Unit Journal</property>
                    <!--<property name="position">1</property> -->
                    <property name="child">
                      <object class="GtkBox" id="unit_journal_stack">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="vexpand">1</property>
                            <property name="focusable">1</property>
                         <!--    <property name="shadow_type">in</property> -->
                            <property name="child">
                              <object class="GtkTextView" id="unit_journal_view">
                                <property name="focusable">1</property>
                                <property name="editable">0</property>
                                <property name="accepts_tab">0</property>
                              </object>
                            </property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="refresh_log_button">
                            <property name="label" translatable="1">Refresh</property>
                            <property name="focusable">1</property>
                            <property name="receives_default">1</property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">Analyze</property>
                    <property name="title" translatable="1">Analyze</property>
               <!--      <property name="position">2</property> -->
                    <property name="child">
                      <object class="GtkBox" id="systemd_analyze_stack">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkBox">
                            <child>
                              <object class="GtkLabel">
                                <property name="label" translatable="1">Total Time:</property>
                                <attributes>
                                  <attribute name="weight" value="medium"></attribute>
                                </attributes>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="time_to_boot">
                                <property name="label" translatable="1">sdf</property>
                                <attributes>
                                  <attribute name="weight" value="medium"></attribute>
                                </attributes>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkScrolledWindow">
                            <property name="vexpand">1</property>
                            <property name="focusable">1</property>
                           <!--  <property name="shadow_type">in</property> -->
                            <property name="child">
                              <object class="GtkTreeView" id="analyze_tree">
                                <property name="focusable">1</property>
                             <!--    <property name="rules_hint">True</property> -->
                                <child internal-child="selection">
                                  <object class="GtkTreeSelection"/>
                                </child>
                              </object>
                            </property>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
    <child type="titlebar">
      <object class="GtkBox">
        <child>
          <object class="GtkHeaderBar" id="left_bar">
  <!--           <property name="has_subtitle">False</property> -->
  <!--           <property name="spacing">0</property> -->
            <child>
              <object class="GtkMenuButton">
                <property name="focusable">1</property>
                <property name="receives_default">1</property>
                <property name="popover">unit_menu_popover</property>
                <child>
                  <object class="GtkLabel" id="unit_menu_label">
                    <property name="label" translatable="1">Services</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkHeaderBar" id="right_bar">
            <property name="hexpand">1</property>
            <child>
              <object class="GtkLabel" id="header_service_label">
                <property name="label" translatable="1">Service Name</property>
                <attributes>
                  <attribute name="weight" value="bold"></attribute>
                </attributes>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="action_buttons">
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="1">Enabled:</property>
                    <attributes>
                      <attribute name="weight" value="bold"></attribute>
                    </attributes>
                  </object>
                </child>
                <child>
                  <object class="GtkSwitch" id="ablement_switch">
                    <property name="focusable">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="start_button">
                    <property name="hexpand">1</property>
                    <property name="label" translatable="1">Start</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="stop_button">
                    <property name="hexpand">1</property>
                    <property name="label" translatable="1">Stop</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                  </object>
                </child>
              </object>
<!--               <packing>
                <property name="pack_type">end</property>
              </packing> -->
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkSizeGroup">
    <widgets>
      <widget name="left_pane"/>
      <widget name="left_bar"/>
    </widgets>
  </object>
</interface>
